<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<rule>
		<from>/css/*</from>
		<to>/html/css/</to>
	</rule>
	<rule>
		<from>/fonts/*</from>
		<to>/html/fonts/</to>
	</rule>
	<rule>
		<from>/images/*</from>
		<to>/html/images/</to>
	</rule>
	<rule>
		<from>/js/*</from>
		<to>/html/js/</to>
	</rule>
	<rule>
		<from>/partials/*</from>
		<to>/html/partials/</to>
	</rule>
	
</urlrewrite>